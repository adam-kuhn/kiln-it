{
  "name": "kiln-it",
  "version": "1.0.0",
  "description": "\"Keep records of clay percent shrinkage, weight and dates of firing for my clay creations\"",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run build:client -- --watch",
    "dev:server": "nodemon server",
    "build": "run-p build:client build:server",
    "build:client": "webpack --config client/webpack.config.js",
    "build:server": "npm run knex migrate:latest",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db": "npm run db:migrate && npm run db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && db",
    "server": "node server",
    "start": "run-s build server",
    "test:watch": "npm test --watchAll",
    "test": "jest --verbose",
    "lint": "eslint client server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilyparkes/kiln-it.git"
  },
  "author": "Emily Coco Parkes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emilyparkes/kiln-it/issues"
  },
  "homepage": "https://github.com/emilyparkes/kiln-it#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "authenticare": "^0.4.7",
    "express": "^4.17.1",
    "knex": "^0.21.6",
    "pg": "^8.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.27.2",
    "sass-loader": "^10.0.4",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.0",
    "superagent": "^6.1.0",
    "sqlite3": "^5.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "supertest": "^5.0.0"
  }
}
